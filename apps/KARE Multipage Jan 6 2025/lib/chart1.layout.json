    },
    "type": "date",
    "tickformat": "%b\n%Y",
    "dtick": "M1",
    "ticklabelmode": "intant",
    "gridcolor": "#fff",
    "zerolinecolor": "#fff"
  },
  "yaxis": {
    "title": {
      "text": "LBS",
      "standoff": 6,
      "font": {
        "size": 12
      }
    },
    "type": "linear",
    "tickformat": "",
